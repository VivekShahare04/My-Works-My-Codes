public class Recursion3 {
    static int fibonacci(int n){
        if(n == 0 || n==1)
        return n;
   
    int firstterm = fibonacci(n-1);
    int nextterm = fibonacci(n-2);

    return (firstterm + nextterm);
    }
    public static void main(String[] args) {
       for (int i = 0; i <= 10; i++) {
        System.out.print(fibonacci(i)+" ");     
       } 
    }
    
}

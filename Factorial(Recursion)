import java.util.*;
public class Recursion2 {
    static int factorial(int n){
        if(n==0){
            return 1;
        }
        int smallans = factorial(n-1);

        int ans = n*smallans;
        return ans;


    }
    public static void main(String[] args) {
        System.out.println(factorial(5));
    }  
    
}
 
